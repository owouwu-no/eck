public abstract class Animal {

    private double weight;
    private int age;

    public Animal(){
        this.weight = 0;
        this.age = 0;
    }

    public Animal(double weight, int age) {
    }

    public void setWeight(double weight) {
        this.weight = weight;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public double getWeight() {
        return weight;
    }

    public int getAge() {
        return age;
    }

    public abstract String eat();
}
